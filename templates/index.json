        "template--15989329985708__978cb35b-1fff-47b1-b64b-e62c7ce9b8e1-16706111210fd25224-0": {
          "type": "slide",
          "settings": {
            "image": "shopify:\/\/shop_images\/Wendy_and_Elle_Website_Photo.png",
            "text_layout": "default-left",
            "custom_color": false,
            "text_color": "#2f5d62",
            "title": "Hello + Welcome!",
            "text": "We are the owners of BC Makers Collective, Elle & Wendy! Swipe right to learn more about us.. you know you want to!",
